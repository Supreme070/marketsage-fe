    generateSmartFallback(question, context, isNigerian) {
        const questionKeywords = question.toLowerCase().split(' ').filter((word)=>word.length > 3);
        const relevantAreas = this.identifyRelevantAreas(questionKeywords);
        return {
            answer: `I'm Supreme-AI, your MarketSage assistant! ${isNigerian ? 'ğŸ‡³ğŸ‡¬ ' : ''}

Based on your question about **"${question}"**, I can help you with:

${relevantAreas.map((area)=>`ğŸ”§ **${area.title}** - ${area.description}`).join('\n')}

**To give you the most relevant advice, tell me:**
â€¢ What specific challenge are you facing?
â€¢ Which MarketSage feature are you working with?
â€¢ What's your main goal right now?

**I specialize in:**
âš¡ Nigerian fintech marketing strategies
ğŸ“± WhatsApp, Email & SMS optimization  
ğŸ¯ Customer segmentation & retention
ğŸš€ Workflow automation setup
ğŸ“Š Performance analytics & insights

**Quick Access:**
â€¢ **Urgent Help**: Type "support" for immediate assistance
â€¢ **Getting Started**: Type "setup" for onboarding guide
â€¢ **Best Practices**: Type "optimize" for improvement tips

${isNigerian ? 'As a Nigeria-focused AI, I understand local market dynamics, CBN regulations, and customer preferences. ' : ''}

What would you like to tackle first?`
        };
    }
    identifyRelevantAreas(keywords) {
        const areas = [
            {
                keywords: [
                    'email',
                    'campaign',
                    'subject'
                ],
                title: 'Email Marketing',
                description: 'Optimize open rates and engagement'
            },
            {
                keywords: [
                    'whatsapp',
                    'message',
                    'chat'
                ],
                title: 'WhatsApp Strategy',
                description: 'Leverage 95% open rates effectively'
            },
            {
                keywords: [
                    'sms',
                    'text',
                    'mobile'
                ],
                title: 'SMS Campaigns',
                description: 'Reliable messaging that works everywhere'
            },
            {
                keywords: [
                    'customer',
                    'segment',
                    'audience'
                ],
                title: 'Customer Intelligence',
                description: 'Smart segmentation and targeting'
            },
            {
                keywords: [
                    'workflow',
                    'automation',
                    'sequence'
                ],
                title: 'Marketing Automation',
                description: 'Set up conversion workflows'
            },
            {
                keywords: [
                    'analytics',
                    'performance',
                    'metrics'
                ],
                title: 'Performance Analytics',
                description: 'Track and optimize campaign results'
            },
            {
                keywords: [
                    'churn',
                    'retention',
                    'lost'
                ],
                title: 'Customer Retention',
                description: 'Prevent churn and increase loyalty'
            },
            {
                keywords: [
                    'conversion',
                    'optimize',
                    'improve'
                ],
                title: 'Conversion Optimization',
                description: 'Increase sales and engagement'
            }
        ];
        return areas.filter((area)=>area.keywords.some((keyword)=>keywords.includes(keyword))).slice(0, 3);
    }
}
const getAIInstance = ()=>{
    // PRIORITY 1: Check for OpenAI-only mode
    if (process.env.USE_OPENAI_ONLY === 'true') {
        if (process.env.OPENAI_API_KEY) {
            console.log('ğŸŒ Using OpenAI (OpenAI-only mode enabled)');
            return new OpenAIIntegration();
        } else {
            console.error('âŒ OpenAI-only mode enabled but no API key found!');
            throw new Error('OpenAI API key required when USE_OPENAI_ONLY=true');
        }
    }
    // PRIORITY 2: Check for Supreme-AI disabled mode
    if (process.env.SUPREME_AI_MODE === 'disabled') {
        if (process.env.OPENAI_API_KEY) {
            console.log('ğŸŒ Using OpenAI (Supreme-AI disabled)');
            return new OpenAIIntegration();
        } else {
            console.log('ğŸ”§ Using Fallback AI (Supreme-AI disabled, no OpenAI key)');
            return new FallbackAI();
        }
    }
    // PRIORITY 3: Check explicit AI_PROVIDER setting
    const aiProvider = process.env.AI_PROVIDER?.toLowerCase() || 'auto';
    switch(aiProvider){
        case 'openai':
            if (process.env.OPENAI_API_KEY) {
                console.log('ğŸŒ Using OpenAI for AI processing');
                return new OpenAIIntegration();
            } else {
                console.warn('OpenAI provider selected but no API key found, falling back to FallbackAI');
                return new FallbackAI();
            }
        case 'fallback':
            console.log('ğŸ”§ Using Fallback AI for AI processing');
            return new FallbackAI();
        default:
            if (process.env.OPENAI_API_KEY) {
                console.log('ğŸŒ Auto-selected OpenAI for AI processing');
                return new OpenAIIntegration();
            } else {
                console.log('ğŸ”§ Auto-selected Fallback AI for AI processing');
                return new FallbackAI();
            }
    }
};

