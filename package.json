{
  "name": "marketsage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "dev:turbo": "next dev -H 0.0.0.0 --turbo",
    "build": "next build",
    "start": "next start -H 0.0.0.0",
    "lint": "bunx biome lint --write && bunx tsc --noEmit",
    "format": "bunx biome format --write",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "db:init": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' src/scripts/init-db.ts",
    "db:reset": "prisma migrate reset --force",
    "db:start": "bash src/scripts/start-local-db.sh",
    "setup": "npm run db:generate && npm run db:init",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "dev:with-db": "npm run db:start && npm run dev",
    "seed-contacts": "npx tsx src/scripts/seedContacts.ts",
    "assign-contacts": "npx tsx src/scripts/assignContactsToCurrentUser.ts",
    "assign-contacts-docker": "npx tsx src/scripts/assignContactsDocker.ts",
    "seed-lists": "npx tsx src/scripts/seedLists.ts",
    "seed-segments": "node src/scripts/seedSegments.js",
    "seed-email-campaigns": "npx tsx src/scripts/seedEmailCampaigns.ts",
    "seed-sms-templates": "npx tsx src/scripts/seedSMSTemplates.ts",
    "seed-sms-campaigns": "npx tsx src/scripts/seedSMSCampaigns.ts",
    "seed-whatsapp-templates": "npx tsx src/scripts/seedWhatsAppTemplates.ts",
    "seed-whatsapp-campaigns": "npx tsx src/scripts/seedWhatsAppCampaigns.ts",
    "seed-workflows": "npx tsx src/scripts/seedWorkflows.ts",
    "seed-all": "./seedAll.sh",
    "create-notification-migration": "node src/scripts/create-notification-migration.js",
    "seed-notifications": "npx tsx src/scripts/seedNotifications.ts"
  },
  "prisma": {
    "seed": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts"
  },
